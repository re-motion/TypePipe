<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <AssemblyInfoFile>$(SolutionDirectory)AssemblyInfoShared.*</AssemblyInfoFile>
    <CompanyName>rubicon IT GmbH</CompanyName>
    <CompanyUrl>http://www.rubicon.eu</CompanyUrl>
    <Copyright>Copyright (c) rubicon IT GmbH, www.rubicon.eu - Licensed under Apache License 2.0</Copyright>
    <ProductName>re-motion Framework: TypePipe (www.re-motion.org)</ProductName>
    <DocumentationFileName>Remotion.TypePipe</DocumentationFileName>
    <DocumentationRootPage>$(SolutionDirectory)Core\Doc\include\GettingStartedWithTypePipe.aml</DocumentationRootPage>
    <DocumentationNamespaceSummaryFiles>$(SolutionDirectory)**\Doc\include\*.NamespaceDoc.xml</DocumentationNamespaceSummaryFiles>
    <DependDBProjectName>re-motion TypePipe</DependDBProjectName>
    <DependDBRetentionTime>10</DependDBRetentionTime>
    <DependDBProjectBranch>Main</DependDBProjectBranch>
    <!-- Required, but can be supplied via commandline parameter in server build and uses a fallback in local build -->
    <DependDBProjectImportNotificationMailAddress>
    </DependDBProjectImportNotificationMailAddress>
    <DependDBTrackedReferences>Remotion.*</DependDBTrackedReferences>
    <DependDBNuGetSources>$(SolutionDirectory)packages</DependDBNuGetSources>
    <DependDBTargetFramework>net45</DependDBTargetFramework>
  </PropertyGroup>
</Project>